<div id="girls" class="uk-child-width-1-2@m" uk-grid uk-scrollspy="cls: uk-animation-fade; target: .uk-card; delay: 500; repeat: true">
  <% @girls.each do |girl| %>
    <div id="<%= dom_id girl %>" class="uk-card uk-card-default uk-card-hover uk-card-body" >
      
      <p>
        <%= image_tag(girl.image.to_s) %>
      </p>

      <p>
        <%= link_to "", girl,class: 'test' %>
      </p>
    </div>
  <% end %>
</div>

